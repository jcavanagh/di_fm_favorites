{
  "manifest_version": 2,

  "name": "DI.fm Favorites",
  "description": "Shows a list of what's playing on your favorite DI.fm stations.",
  "version": "1.1.1",

  "background": {
    "page": "background.html"
  },

  "permissions": [
    "storage",
    "tabs",
    "https://api.audioaddict.com/",
    "http://api.audioaddict.com/",
    "http://listen.di.fm/"
  ],

  "browser_action": {
    "default_icon": "DIFM.png",
    "default_popup": "popup.html"
  }
}